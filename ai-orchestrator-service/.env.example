# Environment variables for AI Orchestrator
# Copy this file to .env and fill in the values

# Gemini API Key (get from https://aistudio.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# Service URLs (defaults are for local development)
MOBILITY_SERVICE_URL=http://localhost:8080
AIR_QUALITY_SERVICE_URL=http://localhost:8081
EMERGENCY_SERVICE_HOST=localhost
EMERGENCY_SERVICE_PORT=9093
CITIZEN_ENGAGEMENT_URL=http://localhost:8084/graphql

# Server config
HOST=0.0.0.0
PORT=8085
